{
    "Stat": {
        "prefix": "stat",
        "body": [
            "stat ${1:kils} ${2|inc,dec,set,mult,div,+=,-=,=,*=,/=|} ${3:value}"
        ],
        "description": "Inserts a 'stat' action with arguments."
    },
    "Globalstat": {
        "prefix": "globalstat",
        "body": [
            "globalstat ${1:kills} ${2|inc,dec,set,mult,div,+=,-=,=,*=,/=|} ${3:value}"
        ],
        "description": "Inserts a 'globalstat' action with arguments."
    },
    "Apply Layout": {
        "prefix": "applyLayout",
        "body": [
            "applyLayout ${1:layoutname}"
        ],
        "description": "Applies a layout."
    },
    "Apply Potion": {
        "prefix": "applyPotion",
        "body": [
            "applyPotion ${1:potionname} ${2:duration} ${3:level} ${4:true|false}"
        ],
        "description": "Applies a potion effect."
    },
    "Enchant Held Item": {
        "prefix": "enchant",
        "body": [
            "enchant ${1:enchantmentname} ${2:level}"
        ],
        "description": "Adds an enchantment to the held item."
    },
    "Show Action Bar": {
        "prefix": "actionBar",
